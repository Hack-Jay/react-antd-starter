{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true // or 'css', don't need import 'antd/dist/antd.css';
      }
    ],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
